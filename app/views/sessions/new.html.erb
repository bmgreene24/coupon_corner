<%= stylesheet_link_tag 'bootstrap' %>

<h1 align class="center">Login</h1>
 <br/>
 <br/>


<%= form_tag sessions_path do %>
    <div class = "form" align = "center" >
    <div class="field">
      <%= label_tag :email %><br />
      <%= text_field_tag :email %>
    </div>
    <div class="field">
      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    </div>
    <div class="actions">
      <%= submit_tag 'Login' %>
    </div>
    </div>
<% end %>

